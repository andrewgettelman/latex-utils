  month = dec,
  year = {2004},
  pages = {1903--1915},
  file = {Full Text PDF:/Users/andrew/Library/Application Support/Zotero/Profiles/tpfh8tv7.default/zotero/storage/PVM3BXGS/Phillips et al. - 2004 - Evaluating Parameterizations in General Circulatio.pdf:application/pdf;Snapshot:/Users/andrew/Library/Application Support/Zotero/Profiles/tpfh8tv7.default/zotero/storage/BQDFNGSP/BAMS-85-12-1903.html:text/html}
