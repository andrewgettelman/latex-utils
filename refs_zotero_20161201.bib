  month = sep,
  year = {2014},
  keywords = {0305 Aerosols and particles,0360 Radiation: transmission and scattering,1622 Earth system modeling,4926 Glacial,dust optical properties,dust size,last glacial maximum,mineral dust,Radiative forcing},
  pages = {541--570},
  file = {albani2014.pdf:/Users/andrew/Dropbox/AGwork/papers/zotero_incoming/albani2014.pdf:application/pdf}
}

