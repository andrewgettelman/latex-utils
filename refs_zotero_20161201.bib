  month = jun,
  year = {2013},
  keywords = {Atmospheric science},
  pages = {355--358},
  file = {Snapshot:/Users/andrew/Library/Application Support/Zotero/Profiles/tpfh8tv7.default/zotero/storage/9ZP3FFXS/nature12278.html:text/html}
}

