  file = {Atmos. Chem. Phys. PDF:/Users/andrew/Library/Application Support/Zotero/Profiles/tpfh8tv7.default/zotero/storage/8RIMQXI3/Gettelman et al. - 2013 - Microphysical process rates and global aerosolclo.pdf:application/pdf;Atmos. Chem. Phys. Snapshot:/Users/andrew/Library/Application Support/Zotero/Profiles/tpfh8tv7.default/zotero/storage/5VUWZI6D/2013.html:text/html}
